<form method="post" action="{{ path('tiny_u_r_l_delete', {'id': tiny_u_r_l.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ tiny_u_r_l.id) }}">
    <button class="btn">Delete</button>
</form>
